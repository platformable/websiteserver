{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "featured_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "categories": {
      "collection": "category",
      "via": "posts",
      "dominant": true
    },
    "is_featured": {
      "type": "boolean"
    },
    "user": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "posts",
      "dominant": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "calendly": {
      "type": "boolean"
    },
    "membership": {
      "type": "enumeration",
      "enum": [
        "free",
        "free_login",
        "paid"
      ],
      "default": "free",
      "required": true
    },
    "excerpt": {
      "type": "text"
    },
    "tags": {
      "collection": "tag",
      "via": "posts"
    },
    "publishing_date": {
      "required": true,
      "type": "date"
    }
  }
}
